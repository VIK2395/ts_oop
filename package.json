{
  "name": "ts",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "compile": "tsc",
    "strategy": "ts-node src/patterns/**/strategy.ts",
    "state": "ts-node src/oop/gof.patterns/**/state.ts",
    "state_enhanced": "ts-node src/oop/gof.patterns/**/state_enhanced.ts",
    "singleton": "ts-node src/oop/gof.patterns/**/singleton.ts",
    "prototype": "ts-node src/oop/gof.patterns/**/prototype.ts",
    "observer": "ts-node src/oop/gof.patterns/**/observer.ts",
    "decorator": "ts-node src/oop/gof.patterns/**/decorator.ts",
    "memento": "ts-node src/oop/gof.patterns/**/memento.ts",
    "proxy": "ts-node src/oop/gof.patterns/**/proxy.ts",
    "chainOfResponsibility": "ts-node src/oop/gof.patterns/**/chainOfResponsibility.ts",
    "factoryMethod": "ts-node src/oop/gof.patterns/**/factoryMethod.ts",
    "abstractFactory": "ts-node src/oop/gof.patterns/**/abstractFactory.ts",
    "builder": "ts-node src/oop/gof.patterns/**/builder.ts",
    "facade": "ts-node src/oop/gof.patterns/**/facade.ts",
    "abstractClasses": "tsc -t es5 src/ts.basics/abstract_classes/main.ts --outDir src/ts.basics/abstract_classes/compiled && node src/ts.basics/abstract_classes/compiled/main.js",
    "interfaces": "tsc -t es5 src/ts.basics/interfaces/main.ts --outDir src/ts.basics/interfaces/compiled && node src/ts.basics/interfaces/compiled/main.js",
    "inheritance": "tsc -t es5 src/ts.basics/inheritance/main.ts --outDir src/ts.basics/inheritance/compiled && node src/ts.basics/inheritance/compiled/main.js",
    "newOperator": "node src/ts.basics/new.operator/main.mjs"
  },
  "author": "viktor.chupak",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}
